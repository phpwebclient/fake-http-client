{
  "name": "webclient/fake-http-client",
  "type": "library",
  "description": "Mock PSR-18 HTTP client",
  "keywords": ["http", "client", "http-client", "mock", "fake"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Dudarev",
      "email": "ivan@ddrv.ru",
      "homepage": "https://ddrv.ru"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "psr/http-factory": "^1.0",
    "nyholm/psr7": "^1.5",
    "phpunit/phpunit": "^6.5 || ^7.5 || 8.5 || 9.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "provide": {
    "psr/http-client-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Webclient\\Fake\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webclient\\Stuff\\Fake\\": "stuff/",
      "Webclient\\Tests\\Fake\\": "tests/"
    }
  },
  "funding": [
    {
      "type": "other",
      "url": "https://www.paypal.me/ddrv"
    }
  ]
}
